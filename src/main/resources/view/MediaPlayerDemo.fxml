<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.demo.MediaPlayerDemo">
   <children>
      <MediaView fx:id="mediaView" fitHeight="200.0" fitWidth="200.0" layoutX="200.0" layoutY="42.0" />
      <FlowPane alignment="CENTER" hgap="20.0" layoutX="200.0" layoutY="275.0" prefHeight="51.0" prefWidth="200.0">
         <children>
            <Button fx:id="stop" mnemonicParsing="false" onAction="#stop" text="STOP" />
            <Button fx:id="play" mnemonicParsing="false" onAction="#play" text="PLAY" />
            <Button fx:id="pause" mnemonicParsing="false" onAction="#pause" text="PAUSE" />
         </children>
      </FlowPane>
      <Slider fx:id="slider" blockIncrement="1.0" layoutX="175.0" layoutY="253.0" majorTickUnit="10.0" minorTickCount="5" onMouseDragReleased="#jump" prefHeight="14.0" prefWidth="250.0" showTickLabels="true" showTickMarks="true" />
   </children>
</AnchorPane>
